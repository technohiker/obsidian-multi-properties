{
  "name": "obsidian-multi-properties",
  "version": "1.2.2",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs dev watch",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:dev": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs dev",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "initialize-test-vault": "node scripts/initialize-test-vault.mjs",
    "install:test": "node scripts/install-plugin-personal.mjs",
    "install:personal": "node scripts/install-plugin-personal.mjs",
    "build-and-install:test": "npm run build && npm run install:test",
    "build-and-install:personal": "npm run build && npm run install:personal",
    "open-personal-vault": "node scripts/open-personal-vault.mjs",
    "setup:test": "npm run build && npm run initialize-test-vault && npm run install:test",
    "setup:dev": "node scripts/initialize-test-vault.mjs",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "technohiker",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/svelte": "^5.1.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vitest/ui": "^1.6.0",
    "builtin-modules": "3.3.0",
    "dotenv": "^16.0.0",
    "esbuild": "0.20.2",
    "esbuild-svelte": "^0.8.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^24.0.0",
    "obsidian": "latest",
    "open": "^9.1.0",
    "postcss-load-config": "^5.0.3",
    "svelte": "^5.0.0-next.1",
    "svelte-preprocess": "^5.1.3",
    "tslib": "2.4.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "overrides": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6"
  }
}
